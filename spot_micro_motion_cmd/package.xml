<?xml version="1.0"?>
<package format="3">
  <name>spot_micro_motion_cmd</name>
  <version>0.1.0</version>
  <description>ROS 2 motion command node for Spot Micro, using i2c_pwm_board + spot_micro_kinematics + keyboard control.</description>

  <maintainer email="you@example.com">you</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- Build/Run deps -->
  <depend>rclcpp</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>Eigen3</depend>
  <depend>spot_micro_kinematics</depend>

  <!-- LaufzeitsabhÃ¤ngigkeiten (executable only) -->
  <exec_depend>i2c_pwm_board</exec_depend>
  <exec_depend>spot_micro_keyboard_cmd</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
